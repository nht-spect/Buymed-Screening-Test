{
  "expo": {
    "name": "quick-order-mobile-app",
    "slug": "quick-order-mobile-app",
    "scheme": "quick-order-mobile-scheme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hoangthinh.quick-order-mobile-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.hoangthinh.quickordermobileapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3df453d1-4922-4a3f-8f78-a9554d931f53"
      }
    },
    "owner": "hoangthinh",
    "plugins": [
      "expo-router"
    ]
  }
}
